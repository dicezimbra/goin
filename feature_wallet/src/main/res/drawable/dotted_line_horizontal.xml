<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="-3dp"
        android:bottom="-1dp"
        android:left="-3dp">
        <shape android:shape="line">

            <solid android:color="@android:color/transparent"/>
            <stroke
                android:width="1dp"
                android:color="#979797"
                android:dashGap="5dp"
                android:dashWidth="6dp"/>
        </shape>
    </item>
</layer-list>